					"arrow-shuffle": "shuffle",
					"arrow-bidir": "arrow-left-right",
					"checkbox": "square-check-big",
					"code-icon": "terminal",
					"controller": "gamepad-2",
					"handshake": "handshake",
					"paint-roller": "paint-roller",
					"sliders": "sliders",
					"subfolders": "folder-tree",
					"star": "star",
					"trophy": "trophy",
					"visibility": "eye",
					"world": "globe"
				}
			},
			"iconsvgxyz": {
				"link": "https://iconsvg.xyz/",
				"used": {
					"arrow-left": "arrow-left",
					"arrow-right": "arrow-right",
					"close": "x"
				}
			}
		},
		"unsplash": {
			"design-sticker-mockup": "https://unsplash.com/photos/jack-of-spade-playing-card-p2E8RQDZ-Zg",
			"design-poster-mockup": "https://unsplash.com/photos/brown-wooden-drawer-beside-green-plant-37j3pDIV9_w"
		}
	},

	"settings": {
		"breakpoint": "36rem",
		"breakpoint-px": "576px",
		"gutter": "24px",
		"toc-min-width": "300px",
		"toc-min-height": "500px"
	},

	"global": {
		"ON": "initial",
		"OFF": " ",

		"sqrt2": "1.4142",
		"sqrt3": "1.7321",

		"layout": {
			"breakpoint": "{{ settings.breakpoint }}",
			"max-content": "60rem",
			"max-content-px": "960px",
			"gutter": "{{ settings.gutter }}",
			"side-border": "3px"
		},

		"size": {
			"2xs": "4px",
			"xs": "8px",
			"s": "12px",
			"m": "16px",
			"l": "32px",
			"xl": "44px",
			"2xl": "56px"
		},

		"typeScale": {
			"n2": "clamp(0.7243rem, 0.6788rem + 0.2204vw, 0.8442rem)",
			"n1": "clamp(0.8511rem, 0.7471rem + 0.5043vw, 1.1253rem)",
			"0": "clamp(1rem, 0.8103rem + 0.9195vw, 1.5rem)",
			"1": "clamp(1.175rem, 0.8623rem + 1.5163vw, 1.9995rem)",
			"2": "clamp(1.3806rem, 0.8933rem + 2.3627vw, 2.6653rem)",
			"3": "clamp(1.6222rem, 0.8899rem + 3.5506vw, 3.5529rem)",
			"4": "clamp(1.9061rem, 0.8327rem + 5.2044vw, 4.736rem)"
		},

		"fontStack": {
			"emoji": ["Twemoji Mozilla", "Apple Color Emoji", "Segoe UI Emoji", "Noto Color Emoji", "EmojiOne Color"],
			"serif": ["Georgia", "Constantia", "Lucida Bright", "Lucidabright", "Lucida Serif", "Lucida", "DejaVu Serif", "Bitstream Vera Serif", "Liberation Serif", "serif"],
			"sans-serif": ["system-ui", "sans-serif"],
			"fantasy": ["Segoe Print", "Bradley Hand", "Chilanka", "TSCu_Comic", "casual", "cursive"],
			"monospace": ["ui-monospace", "Cascadia Code", "Source Code Pro", "Menlo", "Consolas", "DejaVu Sans Mono", "monospace"]
		},

		"anim": {
			"f": 1,
			"delay": "50ms",
			"short": "calc(175ms * var(--anim-f))",
			"medium": "calc(500ms * var(--anim-f))",
			"long": "calc(750ms * var(--anim-f))",
			"eternity": "calc(1000ms * var(--anim-f))"
		},

		"easing": {
			"in": "cubic-bezier(0.75, 0, 0.8, 0.2)",
			"in-out": "cubic-bezier(0.67, 0, 0.33, 1)",
			"out": "cubic-bezier(0.2, 0.8, 0.25, 1)",
			"circ-in": "cubic-bezier(0.835, 0, 0.925, 0.18)",
			"circ-out": "cubic-bezier(0.075, 0.82, 0.165, 1)",
			"bounce": "linear(0, 0.01, 0.042 1.6%, 0.172 3.4%, 0.856 9.5%, 1.092 12.1%, 1.175, 1.237, 1.279, 1.301 16.9%, 1.305, 1.302, 1.292, 1.275 20.3%, 1.222 22.2%, 1.056 27%, 0.986 29.3%, 0.933 31.9%, 0.917, 0.909 34.6%, 0.908, 0.916 38.1%, 0.933 40.1%, 1.004 47%, 1.02, 1.028 52.2%, 1.026 55.9%, 0.999 64.7%, 0.992 69.5% 73.7%, 1.002 86.5%, 1)"
		},

		"C": {
			"inactive": "#808080"
		}
	},

	"defaults": {
		"color-scheme": "light dark",
		"corner": "var(--size-xs)",
		"shadow-color": "0deg 0% 0%",
		"shadow": "0px 0.5px 0.4px hsl(var(--_shadow-color) / 1), 0px 0.9px 0.7px -1.7px hsl(var(--_shadow-color) / 0.82), 0px 4px 3px -3.3px hsl(var(--_shadow-color) / 0.55), 0px 12.2px 9.2px -5px hsl(var(--_shadow-color) / 0.27)",
		"shadow-active": "0px 1px 0.8px -0.8px hsl(var(--_shadow-color) / 0.88), 0px 1.4px 1px -1.2px hsl(var(--_shadow-color) / 0.7), 0px 4px 3px -2.5px hsl(var(--_shadow-color) / 0.52), 0px 11.1px 8.3px -3.7px hsl(var(--_shadow-color) / 0.35), 0px 25px 18.8px -5px hsl(var(--_shadow-color) / 0.17)",

		"main-background": "none",
		"stroke-linecap": "round",
		"focus-outline": "4px dashed var(--_link-color-active)",
		"focus-outline-offset": "2px",
		"text-color": "var(--C-text)",
		"heading-color": "var(--C-heading)",
		"bg-color": "var(--C-canvas)",
		"canvas-color": "var(--C-canvas)",
		"accent-color": "var(--C-accent)",
		"surface-color": "var(--C-surface)",

		"header-bg-color": "transparent",
		"header-link-color": "var(--_link-color)",
		"header-link-color-active": "var(--_link-color-active)",
		"logo-color": "var(--C-heading)",
		"logo-color-active": "var(--C-accent)",
		"footer-special-char": "'⁂'",

		"font-heading-family": "serif",
		"font-heading-weight": "700",
		"font-heading-style": "normal",
		"font-heading-tracking": "0em",
		"font-heading-transform": "none",
		"font-heading-features": "normal",
		"font-heading-shadow": "none",
		"font-heading-size-adjust": "none",
		"font-body-family": "sans-serif",

		"link-color": "currentColor",
		"link-color-active": "var(--C-heading)",
		"link-underline-color": "var(--C-accent)",
		"link-underline-color-active": "var(--C-surface)",
		"link-underline-offset": "0.25em",
		"link-underline-offset-active": "-0.25em",
		"link-skip-ink": "auto",
		"link-skip-ink-active": "none",
		"link-decoration-thickness": "clamp(2px, 0.125em, 4px)",
		"link-decoration-thickness-active": "0.5em",

		"cta-padding-block": "var(--size-2xs)",
		"cta-padding-inline": "var(--size-s)",
		"cta-icon-size": "1.5em",
		"cta-text-color": "var(--C-text)",
		"cta-text-color-active": "var(--C-text)",
		"cta-text-color-pressed": "var(--C-surface)",
		"cta-bg-color": "var(--C-surface)",
		"cta-bg-color-active": "var(--C-surface)",
		"cta-bg-color-pressed": "var(--C-text)",
		"cta-border-radius": "var(--_corner)",
		"cta-border-width": "1px",
		"cta-border-color": "var(--C-heading)",
		"cta-border-color-active": "var(--C-accent)",
		"cta-border-color-pressed": "var(--C-text)",
		"cta-box-shadow": "none",
		"cta-box-shadow-active": "inset 0 0 0 1px var(--C-accent)",
		"cta-box-shadow-pressed": "inset 0 0 0 1px var(--C-surface)",

		"card-padding": "var(--size-m)",
		"card-corner": "calc(var(--_card-padding) + var(--_corner))",
		"card-heading-color": "var(--_link-color)",
		"card-heading-color-active": "var(--C-heading)",
		"card-text-color": "var(--_cta-text-color)",
		"card-text-color-active": "var(--_cta-text-color-active)",
		"card-bg-color": "var(--_cta-bg-color)",
		"card-bg-color-active": "var(--_cta-bg-color-active)",
		"card-border-color": "var(--_cta-border-color)",
		"card-border-color-active": "var(--_cta-border-color-active)",
		"card-border-width": "var(--_cta-border-width)",

		"hero-rule-color": "color-mix(in hsl, var(--C-text), transparent 75%)",

		"signoff-heading-color": "var(--_card-heading-color)",
		"signoff-text-color": "var(--_card-text-color)",
		"signoff-background": "var(--_card-bg-color)",
		"signoff-border": "none"
	},


{{ /set -}}
{{ inlineCSS |> await cssmin }}
</style>

<script>
{{- set inlineJS -}}
	document.addEventListener('DOMContentLoaded', () => {
		document.querySelector(`[data-interaction="projector-power"]`).addEventListener('click', () => {
			const pwr = document.querySelector(`[data-interacted="projector-power"]`);
			if (pwr) pwr.setAttribute('data-power', pwr.getAttribute('data-power') !== 'off' ? 'off' : 'on');
		});

		// Hacky AF but it ensures the animation does not fire if JS doesn't work, or if the user has asked for reduced motion
		if (window.matchMedia(`(prefers-reduced-motion: no-preference)`).matches) {
			const svgTV = document.getElementById('illustration-television');
			const svgBooks = document.getElementById('illustration-books');
			svgTV.querySelector('defs').innerHTML += `<animate 
				xlink:href="#static"
				attributeName="x"
				values="0;-85;127;-12;199;-73"
				dur="0.25s"
				repeatCount="indefinite"
			/>
			<animate 
				xlink:href="#static"
				attributeName="y"
				values="0;231;63;-97;56;157;-201"
				dur="0.33s"
				repeatCount="indefinite"
			/>`;

			svgBooks.querySelector('defs').innerHTML += `<animate 
				xlink:href="#feTurb2"
				attributeName="scale"
				values="15;25;15"
				dur="2s"
				repeatCount="indefinite"
			/>`;
		}
		
		document.querySelector(`[data-interaction="tv-power"]`).addEventListener('click', () => {
			const pwr = document.querySelector(`[data-interacted="tv-power"]`);
			if (pwr) pwr.setAttribute('data-power', pwr.getAttribute('data-power') !== 'off' ? 'off' : 'on');
		});

		document.querySelector(`[data-interaction="hikepal-power"]`).addEventListener('click', () => {
			const pwr = document.querySelector(`[data-interacted="hikepal-power"]`);
			if (pwr) pwr.setAttribute('data-power', pwr.getAttribute('data-power') !== 'off' ? 'off' : 'on');
		});

		const konami = ['u','u','d','d','l','r','l','r','b','a'];
		let userSeq = [];
		let hasSuccess = false;
		document.querySelector(`[data-interacted="controller"]`).addEventListener('click', (e) => {
			if (hasSuccess) { return; }
			const target = e.target.closest('[data-interaction]');
			if (!target) { return; }
			const input = target.getAttribute('data-input');
			const nextCorrect = konami[userSeq.length];
			if (input !== nextCorrect) {
				userSeq = [];
				return;
			} else {
				userSeq.push(input);
			}
			if (userSeq.join('.') === konami.join('.')) {
				document.querySelector(`[data-illu-part="health"]`).removeAttribute('transform');
				hasSuccess = true;
			}
		});

		document.getElementById('nodesc').addEventListener('click', function(e) {
			const target = e.currentTarget;
			const wasPressed = target.getAttribute('aria-pressed') === 'true';
			const isPressed = !wasPressed;
			const allFloatsters = Array.from(document.querySelectorAll('.fullsection-content .floatster'));
			if (isPressed) {
				allFloatsters.forEach(f => f.setAttribute('data-image-only', ''));
			} else {
				allFloatsters.forEach(f => f.removeAttribute('data-image-only'));
			}
		});
	});
{{ /set -}}
{{ inlineJS |> await jsmin }}
</script>

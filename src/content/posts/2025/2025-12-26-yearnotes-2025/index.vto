		@keyframes --line-anim {
			0% { stroke-dasharray: 1 1; stroke-dashoffset: 1; stroke-width: 16; stroke-opacity: 0.5; filter: blur(0); }
			99%, 100% { stroke-dasharray: 0.5 1; stroke-dashoffset: -1; stroke-width: 0; stroke-opacity: 1; filter: blur(8px);}
		}
		.fullsection-illustration-spin-anim {
			animation: --spin-anim calc(10s * var(--FS-factor, 1)) linear infinite;
		}
		@keyframes --spin-anim {
			to { transform: rotate(360deg); }
